<!DOCTYPE html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html$if(lang)$ lang="$lang$" $endif$$if(dir)$ dir="$dir$" $endif$ class="no-js">
  <!--<![endif]-->

  <head>
    <meta charset="utf-8">
    <meta name="generator" content="Bluefish 2.2.10">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1, user-scalable=yes">
    $if(theme-color)$
    <meta name="theme-color" content="$theme-color$">
    $endif$

    $if(site_name)$
    <meta property="og:site_name" content="$site_name$">
    $endif$

    <title>$if(title-prefix)$$title-prefix$ – $endif$$pagetitle$</title>
    $if(description)$
    <meta name='description' content='$description$'>
    <meta property="og:description" content="$description$">
    $else$
    $if(subject)$
    <meta name='description' content='$subject$'>
    <meta property="og:description" content="$subject$">
    $else$
    $if(subtitle)$
    <meta name='description' content='$subtitle$'>
    $endif$
    $endif$
    $endif$
    $if(keywords)$
    <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
    $endif$
    $if(author-meta)$
    <meta name="author" content="Unknown">
    $endif$

    <meta property="og:type" content="article">
    $if(lang)$
    <meta property="og:locale" content="$lang$">
    $endif$
    $if(date-meta)$
    <meta name="dcterms.date" content="$date-meta$">
    <meta property="article:published_time" content="$date-meta$">
    $else$
    $if(date)$
    <meta name="dcterms.date" content="$date$">
    <meta name="article:published_time" content="$date$">
    $endif$
    $endif$

    $if(quotes)$
    <style>
      q {
        quotes: "“""”""‘""’";
      }
    </style>
    $endif$
    $if(highlighting-css)$
    <style>
      $highlighting-css$
    </style>
    $endif$
    $if(css)$
    $for(css)$
    <link rel="stylesheet" href="$css$" type="text/css">
    $endfor$$endif$
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
<script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv-printshiv.min.js"></script>
<script src="//oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
<![endif]-->
    $for(header-includes)$
    $header-includes$
    $endfor$
  </head>


  <body id="body" data-spy="scroll" data-target="#navbar-toc" data-offset="0">
    <div id="$idprefix$Top" class="clearfix container-fluid">
      <!--include before (eg. banner/masthead) -->
      $for(include-before)$
      $include-before$
      $endfor$
      <div class="row flex-xl-nowrap" role="main">
        <main class="col-xs-12 py-2 justify-content-center">
          <article class="py-2 pl-2 mx-3 article-main" itemscope itemtype="https://schema.org/Article">
            <meta itemprop="headline" content="$pagetitle$">
            $if(description)$
            <meta itemprop="description" content="$description$">
            $endif$
            $if(date)$
            <meta itemprop="datePublished" content="$date$">
            $endif$
            $if(lang)$
            <meta itemprop="inLanguage" content="$lang$">
            $endif$
            <header class="article-header">
              <h1 class="title article-title" itemprop="headline">$title$</h1>
              $if(subtitle)$
              <h2 class="subtitle">$subtitle$</h2>
              $endif$
              <div class="flex-wrap article-meta">
                $if(authors)$
                <div class="container-fluid article-authors">
                  <ul class="row nav-authors">
                    $for(authors)$
                    <li class="col-md-4 item-author" itemprop="author" itemscope itemtype="http://schema.org/Person">
                      $if(authors.name)$
                      <span class="fn author-name" itemprop="name">$authors.name$</span>$endif$
                      $if(authors.affiliation)$
                      <span itemprop="affiliation" itemscope itemtype="http://schema.org/Organization" class="author-affiliation"><span itemprop="name" class="affiliation">$authors.affiliation$</span></span>
                      $endif$
                    </li>
                    $endfor$
                  </ul>
                </div>
                $endif$
                <div class="clearfix container-fluid article-summary">
                  $if(abstract)$
                  <div class="abstact-title">
                    <h2>$if(abstact-title)$$abstract-title$$else$Abstract$endif$</h2>
                  </div>
                  <p class="abstract">
                    $abstract$
                  </p>
                  $endif$
                  $if(keywords)$
                  <div itemprop="keywords" class="article-keywords">
                    <span class="keywords-title">
                      <h7>$if(keywords-title)$$keywords-title$$else$Keywords$endif$</h7>
                    </span>
                    $for(keywords)$<span class="article-keywords">$keywords$</span>$sep$, $endfor$
                  </div>
                  $endif$
                </div>
              </div>
            </header>

            <section class="row my-3 article-section">
              $if(toc)$
              <div class="col-md-2 d-print-none">
                <aside class="d-none d-md-block sidebar-toc">
                  <nav class="list-group article-toc" id="navbar-toc">
                    $if(nav-contentsname)$
                    <header class="navbar-brand">
                      <h7 class="toc-title"><i class="fas fa-file-alt"></i> $nav-contentsname$</h7>
                    </header>
                    $endif$
                    $table-of-contents$
                  </nav>
                </aside>
              </div>
              $endif$

              <div class="col-md-10 px-3 article-content">
                <div class="container-fluid row article-text" itemprop="text">

                  $body$

                </div>
              </div>
            </section>
          </article>
        </main>
      </div>
      <!-- include after -->
      $for(include-after)$
      $include-after$
      $endfor$

    </div>

    $if(math)$
    <script type="text/x-mathjax-config">
      MathJax.Hub.Config({
    extensions: ["tex2jax.js"],
    jax: ["input/TeX", "output/HTML-CSS"],
    tex2jax: {
      displayMath: [ ['$$','$$'], ["\\[","\\]"] ],
      processEscapes: true
    },
    "HTML-CSS": { fonts: ["TeX"] },
    TeX:{
      extensions: ["AMSmath.js", "AMSsymbols.js", "mhchem.js"],
      mhchem: { legacy: true }
    }
  });
</script>
    $math$
    $endif$


    <script async defer src="https://use.fontawesome.com/releases/v5.5.0/js/all.js" integrity="sha384-GqVMZRt5Gn7tB9D9q7ONtcp4gtHIUEW/yG7h98J7IpE3kpi+srfFyyB/04OV6pG0" crossorigin="anonymous"></script>

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script defer src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
    <script  src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    <script>
    $$('#navbar-toc ul').addClass('nav');// to add 'nav' to toc
    $$('#navbar-toc ul li').addClass('nav-item');// to add 'nav-item' to toc
    $$('#navbar-toc ul li a').addClass('nav-link');// to add 'nav-link' to toc
    $$('[data-spy="scroll"]').each(function () {
      var $$spy = $$(this).scrollspy('refresh')
    });
    </script>
  </body>

  </html>
